{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "watch": "npx squint watch",
    "preview": "pnpm vite preview",
    "deploy-composite": "export VITE_CERAMIC_API=${CERAMIC_API} && export DID_PRIVATE_KEY=${DID_PRIVATE_KEY} && echo ${VITE_CERAMIC_API} && npx squint run src/dev/dev.cljs",
    "dev": "concurrently --kill-others \"source ./.env.development && ./wait_for_ceramic.sh && pnpm deploy-composite && pnpm dlx vite\" \"source ./ceramic.sh && ceramic_daemon\" \"npx hardhat node\" ",
    "build": "vite build",
    "serve": "vite preview",
    "graphql-server": "source ./ceramic.sh && npx composedb graphql:server --ceramic-url=http://localhost:7007 --graphiql src/__generated__/merged-rt.json --did-private-key=${DID_PRIVATE_KEY} --port=5005",
    "postinstall": "patch --forward ./node_modules/squint-cljs/package.json ./vite-plugin/package.json.patch || echo "
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.24.7",
